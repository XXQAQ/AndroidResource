<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--白色(如果觉得不够白可自定义)-->
    <color name="white">@android:color/white</color>
    <!--建议自定义您的背景色-->
    <color name="background">#F4F4F4</color>
    <!--建议自定义您的辅色-->
    <color name="secondary">@android:color/black</color>
    <!--高亮色-->
    <color name="highlight">#23070707</color>

    <!--常用字体颜色-->
    <color name="text_normal">#494949</color>
    <!--比常用字体更醒目的颜色-->
    <color name="text_black">#070707</color>
    <!--提示字体颜色-->
    <color name="text_hint">#ADADAD</color>

</resources>
