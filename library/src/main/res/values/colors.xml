<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--使用最多的颜色，亮色主题下等同于white-->
    <color name="colorWidely">@android:color/white</color>
    <!--建议在您的项目中自定义主色-->
    <color name="colorPrimary"></color>
    <!--建议在您的项目中自定以辅色-->
    <color name="colorSecondary">@android:color/black</color>
    <!--建议在您的项目自定义您的控件背景色-->
    <color name="colorBackground">#F4F4F4</color>
    <!--高亮色-->
    <color name="highlight">#23070707</color>
    <!--禁用色-->
    <color name="disable">#E5E5E5</color>
    <!--黑玻璃色-->
    <color name="whiteGlass">#45000000</color>
    <!--白玻璃色-->
    <color name="blackGlass">#45ffffff</color>

    <!--淡色字体-->
    <color name="text_weak">#ADADAD</color>
    <!--常用字色-->
    <color name="text_normal">#494949</color>
    <!--深色字体-->
    <color name="text_strong">#070707</color>

</resources>
